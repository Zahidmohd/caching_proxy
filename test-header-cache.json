{
  "server": {
    "port": 3000,
    "origin": "https://dummyjson.com"
  },
  "cache": {
    "enabled": true,
    "defaultTTL": 300,
    "maxEntries": 1000,
    "maxSizeMB": 100,
    "compression": {
      "enabled": false,
      "method": "none"
    },
    "cacheKeyHeaders": ["accept-language", "accept-encoding"]
  },
  "logging": {
    "enabled": false
  }
}

